rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // ULTRA PERMISSIVE RULES - FOR EMERGENCY DEVELOPMENT USE ONLY
    // These rules allow ALL access to ALL documents for ALL users
    // WARNING: DO NOT USE IN PRODUCTION!
    
    match /{document=**} {
      allow read, write: if true;
    }
  }
}



























