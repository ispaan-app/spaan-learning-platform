{
  "name": "ispaan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:security": "npm audit --audit-level moderate",
    "test:performance": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "setup:env": "node scripts/setup-comprehensive-env.js",
    "fix:permissions": "node scripts/fix-firebase-permissions.js",
    "validate:rules": "node scripts/validate-firestore-rules.js",
    "emergency:firestore": "node scripts/emergency-firestore-fix.js",
    "enable:ai": "node scripts/enable-ai-features.js",
    "optimize:config": "node scripts/optimize-configuration.js",
    "init:super-admin-messages": "node scripts/init-super-admin-messages.js",
    "analyze:bundle": "ANALYZE=true npm run build",
    "setup:firebase": "npm run setup:env && npm run validate:rules && npm run fix:permissions",
    "setup:complete": "npm run setup:env && npm run enable:ai && npm run validate:rules && npm run fix:permissions",
    "setup:optimized": "npm run optimize:config && npm run setup:complete",
    "setup:test": "npm install && npx playwright install",
    "precommit": "npm run lint:fix && npm run test:ci",
    "prepare": "husky install"
  },
